    "@types/react-dom": "^16.9.5",
    "cross-env": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@babel/node": "^7.8.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
